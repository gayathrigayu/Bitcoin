{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
     "sassgelcustom": "node-sass src/sass/app.scss  public/build/css/bundle.css  --source-map true --output-style compressed --source-map-contents true",
    "build": "sassgelcustom rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sv-bootstrap-dropdown": "^0.4.4",
    "svelte": "^3.44.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "sveltestrap": "^5.6.3"
  }
}
